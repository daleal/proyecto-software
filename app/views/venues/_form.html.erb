<%= form_with(model: venue, local: true) do |form| %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :venue_name %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address, id: :venue_address %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_field :description, id: :venue_description %>
  </div>

  <div class="actions">
    <%= form.submit submit_value%>
  </div>
<% end %>
